{
  "name": "@my-pages/content-fetcher",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts",
      "import": "./dist/schemas.mjs",
      "require": "./dist/schemas.js"
    }
  },
  "files": [
    "./dist/**"
  ],
  "description": "Fetch content from external sources (Zenn, Hatena, SpeakerDeck) and generate JSON",
  "scripts": {
    "build": "tsup",
    "generate": "tsx src/cli.ts",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "devDependencies": {
    "tsup": "catalog:",
    "tsx": "^4.19.4",
    "typescript": "catalog:",
    "vitest": "^4.0.17"
  },
  "dependencies": {
    "@my-pages/errors": "workspace:*",
    "feed": "^4.2.2",
    "rss-parser": "^3.13.0",
    "zod": "catalog:"
  }
}
